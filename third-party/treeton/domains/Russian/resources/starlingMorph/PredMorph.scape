  }

  rule r6 {
   {Token: kind=word, lang=cyr}{Token: kind=punctuation, subkind=dashpunct}{Token: kind=word, lang=cyr}{Token: kind=punctuation, string="."}
   ->
  }

  rule r7 {
   {Token: kind=word, lang=cyr}{Token: kind=punctuation, string="."}
   ->
  }

