   {Token: kind=word, lang=cyr}{Token: kind=punctuation, subkind=dashpunct}{Token: kind=word, lang=cyr}{Token: kind=punctuation, subkind=dashpunct}{Token: kind=word, lang=cyr}{Token: kind=punctuation, string="."}
