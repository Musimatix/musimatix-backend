{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Keywords for suggesting songs' titles",
  "self": {
    "vendor": "com.musimatix",
    "name": "suggest_title_request",
    "format": "jsonschema",
    "version": "1-0-0"
  },
  "type": "object",
  "properties": {
    "object": {
      "type": "string"
    },
    "version": {
      "type": "string"
    },
    "suggestTitle": {
      "type": "object",
      "properties": {
        "limit": {
          "type": "integer",
          "minimum": 1
        },
        "keywords": {
          "type": "string"
        }
      },
      "required": ["keywords"],
      "additionalProperties": false
    }
  },
  "required": ["object", "suggestTitle"],
  "additionalProperties": false
}
